      //Text15.Text=GetValue(&quot;Master.QTY&quot;);
    }
    
    private string GetValue(string column){
      string v= Report.GetColumnValue(column).ToString();
      if(v.Split('.').Length&gt;1){
        if(v.Split('.')[1]==&quot;0&quot;) v=v.Split('.')[0];
      }
      return v;
    }
  }
